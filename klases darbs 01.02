void seconduzd() {
	int mark;
	cout << "enter students mark in %\n";
	cin >> mark;
	if (mark < 40)
	{
		cout << "students grade-F";
	}
	if (mark >= 40 && mark < 60) {
		cout << "students grade-E";
	}
	if (mark >= 60 && mark < 70) {
		cout << "students grade-D";
	}
	if (mark >= 70 && mark < 80) {
		cout << "students grade-C";
	}
	if (mark >= 80 && mark < 90) {
		cout << "students grade-B";
	}
	if (mark >= 90 && mark <= 100) {
		cout << "students grade-A";
	}
	if (mark > 100 || mark < 0)
		cout << "enter valid value";


}
