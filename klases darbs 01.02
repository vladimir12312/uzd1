#include<iostream>//piepildīnam kodu ar pamatbibleoteku
using namespace std;//lai nerakstīt prefiksu std:: katru reizi
int main() {
	int x, y, z;
	cout << "enter length of triangle sides";
	cin >> x >> y >> z;
	if (x == y == z) {
		cout << "triangle is regular";
	}
	else if (x == y || y == z || x == z) {
		cout << "triangle is esoscale";
	}//vienadsanu
	else {
		cout << "triangle isnt esoscale or regular";
	}
}

